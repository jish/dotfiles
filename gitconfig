[color]
  status = auto
  diff = auto
[color "status"]
  untracked = cyan
  changed = green
  added = yellow
[alias]
  co = checkout
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
[core]
	excludesfile = /Users/josh/.gitignore
	editor = mate -w
